type MessageStatus = variant {
  MessageHandlerFailed;
  BurnFailed;
  Succeeded;
  ConsumeMessageFailed;
  SendMessageFailed;
  MintFailed;
};
type Result = variant { Ok : nat; Err : MessageStatus };
service : () -> {
  burn : (principal, nat) -> (Result);
  handle_message : (principal, vec nat) -> (Result);
  mint : (vec nat) -> (Result);
}