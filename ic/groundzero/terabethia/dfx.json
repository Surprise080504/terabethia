{
  "canisters": {
    "tera": {
      "build": "bash build.sh tera",
      "candid": "src/tera/tera.did",
      "wasm": "target/wasm32-unknown-unknown/release/tera.wasm",
      "type": "custom"
    },
    "eth_proxy": {
      "build": "bash build.sh eth_proxy",
      "candid": "src/eth_proxy/proxy.did",
      "wasm": "target/wasm32-unknown-unknown/release/eth_proxy.wasm",
      "type": "custom"
    },
    "token": {
		  "build": "cargo build --target wasm32-unknown-unknown --package token --release",
		  "candid": "src/token/token.did",
		  "wasm": "target/wasm32-unknown-unknown/release/token.wasm",
		  "type": "custom"
	  }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "dfx": "0.8.1",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}